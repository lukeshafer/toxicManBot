{
	"name": "toxicmanbot",
	"version": "1.5.0",
	"description": "Just a toxic little friend for you <3",
	"main": "./build/bot.js",
	"scripts": {
		"start": "pnpm run init && pnpm run bot",
		"init": "pnpm run build && pnpm run deployCommands",
		"build": "esbuild ./src/*.ts --bundle --platform=node --outdir=build ",
		"deployCommands": "node ./build/deploy-commands.js",
		"dev": "pnpm run build -- --watch ",
		"bot": "screen -dmS toxicMan pnpm run startBot",
		"startBot": "node .",
		"stop": "screen -S toxicMan -X quit",
		"restart": "pnpm run stop && pnpm run start",
		"api": "node ./build/api/_api.js",
		"buildapi": "esbuild ./src/data/_api.ts --bundle --external:./node_modules/* --platform=node --outdir=build/api --format=esm"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lukeshafer/toxicManBot.git"
	},
	"author": "Luke Shafer",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/lukeshafer/toxicManBot/issues"
	},
	"homepage": "https://github.com/lukeshafer/toxicManBot#readme",
	"dependencies": {
		"@discordjs/builders": "^0.15.0",
		"@discordjs/rest": "^0.5.0",
		"discord-api-types": "^0.36.3",
		"discord.js": "^13.9.1",
		"dotenv": "^16.0.1",
		"ejs": "^3.1.8",
		"express": "^4.18.1",
		"express-svelte": "^1.0.8",
		"http-server": "^14.1.1",
		"svelte": "^3.49.0"
	},
	"devDependencies": {
		"@tsconfig/svelte": "^3.0.0",
		"@types/express": "^4.17.13",
		"esbuild": "^0.14.50",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4"
	}
}
