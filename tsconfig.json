{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"compilerOptions": {
		"module": "ES2022",
		"esModuleInterop": true,
		"target": "es6",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "build",
		"noImplicitAny": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*"],
	"lib": ["es2021"]
}
